{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "launchweek.dev",
  "colors": {
    "primary": "#8957E5",
    "light": "#8957E5",
    "dark": "#8957E5"
  },
  "favicon": "/img/lw-icon-dark.svg",
  "navigation": {
    "groups": [
      {
        "group": "launchweek.dev",
        "pages": [
          "home",
          "lw/new",
          "lw/faq"
        ]
      },
      {
        "group": "launch weeks",
        "pages": [
          "lw/2025",
          "lw/2024",
          "lw/2023",
          "lw/2022",
          "lw/2021"
        ]
      },
      {
        "group": "best practices",
        "pages": [
          "n/supabase",
          "n/resend"
        ]
      },
      {
        "group": "maintainers",
        "pages": [
          "n/kiwicopple",
          "n/fmerian"
        ]
      }
    ]
  },
  "styling": {
    "codeblocks": "system"
  },
  "logo": {
    "light": "/img/lw-icon-light.svg",
    "dark": "/img/lw-icon-dark.svg"
  },
  "appearance": {
    "default": "dark"
  },
  "background": {
    "color": {
      "light": "#FFFFFF",
      "dark": "#0D1117"
    }
  },
  "navbar": {
    "primary": {
      "type": "github",
      "href": "https://github.com/supabase-community/launchweek.dev"
    }
  },
  "seo": {
    "indexing": "all"
  },
  "footer": {
    "socials": {
      "github": "https://git.new/lw",
      "x": "https://x.com/launchweekdev"
    },
    "links": [
      {
        "header": "launchweek.dev",
        "items": [
          {
            "label": "README.md",
            "href": "https://launchweek.dev/"
          },
          {
            "label": "CONTRIBUTING.md",
            "href": "https://launchweek.dev/lw/new"
          },
          {
            "label": "FAQ.md",
            "href": "https://launchweek.dev/lw/faq"
          }
        ]
      },
      {
        "header": "maintainers",
        "items": [
          {
            "label": "Paul Copplestone",
            "href": "https://x.com/kiwicopple"
          },
          {
            "label": "Flo Merian",
            "href": "https://x.com/fmerian"
          }
        ]
      }
    ]
  },
  "fonts": {
    "heading": {
      "family": "JetBrains Mono",
      "weight": 400
    },
    "body": {
      "family": "Inter"
    }
  },
  "redirects": [
    {
      "source": "/lw/MEGA",
      "destination": "https://launchweek.dev/lw/2024/mega"
    },
    {
      "source": "/content/rorstro",
      "destination": "/n/rorstro"
    },
    {
      "source": "/n",
      "destination": "/n/forward"
    }
  ]
}