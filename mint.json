{
  "$schema": "https://mintlify.com/schema.json",

  "name": "launchweek.dev",

  "logo": {
    "light": "/assets/lw-icon-light.svg",
    "dark": "/assets/lw-icon-dark.svg"
  },

  "favicon": "/assets/lw-icon-dark.svg",

  "colors": {
    "primary": "#8957E5",
    "light": "#8957E5",
    "dark": "#8957E5",
    "background": {
      "light": "#FFFFFF",
      "dark": "#0D1117"
    }
  },

  "modeToggle": {
    "default": "dark"
  },

  "font": {
    "headings": {
      "family": "JetBrains Mono",
      "weight": 400
    },
    "body": {
      "family": "Inter"
    }
  },

  "topbarCtaButton": {
    "type": "github",
    "url": "https://github.com/supabase-community/launchweek.dev"
  },

  "sidebar": {
    "items": "undecorated"
  },

  "codeBlock": {
    "mode": "auto"
  },

  "navigation": [
    { "group": "launchweek.dev", "pages": ["home", "CONTRIBUTING"] },
    {
      "group": "launch weeks",
      "pages": ["lw/2024", "lw/2023", "lw/2022", "lw/2021"]
    },
    { "group": "best practices", "pages": ["content/supabase"] },
    {
      "group": "maintainers",
      "pages": ["content/kiwicopple", "content/fmerian"]
    }
  ],

  "redirects": [
    {
      "source": "/lw/MEGA",
      "destination": "/lw/2024/mega"
    }
  ],

  "seo": {
    "indexHiddenPages": true
  },

  "footer": {
    "socials": {
      "github": "https://git.new/lw",
      "x": "https://x.com/launchweekdev"
    },
    "links": [
      {
        "title": "launchweek.dev",
        "links": [
          { "label": "README.md", "url": "https://launchweek.dev/" },
          {
            "label": "CONTRIBUTING.md",
            "url": "https://launchweek.dev/CONTRIBUTING"
          }
        ]
      },
      {
        "title": "maintainers",
        "links": [
          {
            "label": "Paul Copplestone",
            "url": "https://x.com/kiwicopple"
          },
          { "label": "Flo Merian", "url": "https://x.com/fmerian" }
        ]
      }
    ]
  }
}
